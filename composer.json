{
    "name": "zoho-mail/craft-zoho-mail",
    "description": "Thirdpart mail plugin for business e-mail",
    "type": "craft-plugin",
    "license": "mit",
    "version": "1.3",
    "support": {
        "email": "support@zohomail.com",
        "issues": "https://github.com/zohomail/craft-zohomail/issues?state=open",
        "source": "https://github.com/zohomail/craft-zohomail",
        "docs": "https://github.com/zohomail/craft-zohomail",
        "rss": "https://github.com/zohomail/craft-zohomail/releases.atom"
    },
    "require": {
        "php": ">=8.0.2",
        "craftcms/cms": "^4.0.0|^5.0.0"
    },
    "autoload": {
        "psr-4": {
            "zohomail\\craftzohomail\\": "src/"
        }
    },
    "extra": {
        "handle": "zoho-mail",
        "name": "Zoho Mail",
        "developer": "Zoho Mail",
        "documentationUrl": "https://github.com/zohomail/craft-zohomail",
        "class": "zohomail\\craftzohomail\\ZohoMail"
    },
    "config": {
        "sort-packages": true,
        "platform": {
            "php": "8.0.2"
        },
        "allow-plugins": {
            "yiisoft/yii2-composer": true,
            "craftcms/plugin-installer": true
        }
    }
}

